<Ui xmlns="http://www.blizzard.com/wow/ui/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/
    http://wowprogramming.com/FrameXML/UI.xsd">

<Frame name="MyCustomDropDownMenu" inherits="UIDropDownMenuTemplate" virtual="true">
  <Frames>
    <Frame name="$parentLeft">
      <Size x="16" y="16"/>
      <Anchors>
        <Anchor point="TOPLEFT"/>
      </Anchors>
      <Texture file="Interface\Glues\CharacterCreate\CharacterCreate-LabelFrame"/>
      <TexCoords left="0" right="0.1953125" top="0.5" bottom="1"/>
    </Frame>
    <Frame name="$parentMiddle">
      <Size x="1" y="16"/>
      <Anchors>
        <Anchor point="LEFT" relative="$parentLeft" relativePoint="RIGHT"/>
        <Anchor point="RIGHT" relative="$parentRight" relativePoint="LEFT"/>
      </Anchors>
      <Texture file="Interface\Glues\CharacterCreate\CharacterCreate-LabelFrame"/>
      <TexCoords left="0.1953125" right="0.8046875" top="0.5" bottom="1"/>
    </Frame>
    <Frame name="$parentRight">
      <Size x="16" y="16"/>
      <Anchors>
        <Anchor point="TOPRIGHT"/>
      </Anchors>
      <Texture file="Interface\Glues\CharacterCreate\CharacterCreate-LabelFrame"/>
      <TexCoords left="0.8046875" right="1" top="0.5" bottom="1"/>
    </Frame>
    <FontString name="$parentText" inherits="GameFontHighlight" text="" justifyH="LEFT">
      <Anchors>
        <Anchor point="TOPLEFT" relative="$parentLeft" relativePoint="TOPLEFT" x="0" y="-2"/>
        <Anchor point="BOTTOMRIGHT" relative="$parentRight" relativePoint="BOTTOMRIGHT" x="0" y="2"/>
      </Anchors>
    </FontString>
    <Button name="$parentButton" inherits="UIDropDownMenuButtonTemplate" virtual="true"/>
  </Frames>
</Frame>

</Ui>